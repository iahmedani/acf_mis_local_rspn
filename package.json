{
  "name": "acf_mis_local_rspn",
  "version": "4.0.5",
  "description": "ACF MIS FOR PINS FOR RSPN",
  "main": "main.js",
  "scripts": {
    "start": "electron . ",
    "publish": "npm run clean && build --win -p always ",
    "clean": "rm -rf ./dist",
    "postinstall": "electron-builder install-app-deps",
    "pack": "electron-builder --dir",
    "dist": "electron-builder --ia32 --x64 -w"
  },
  "build": {
    "nsis": {
      "oneClick": false,
      "perMachine": true,
      "allowToChangeInstallationDirectory": true
    },
    "publish": [
      {
        "provider": "github",
        "owner": "iahmedani",
        "repo": "acf_mis_local_rspn"
      }
    ],
    "appId": "com.github.iahmedani.aahmislocalapprspn",
    "win": {
      "icon": "build/win/logo.ico",
      "target": [
        {
          "target": "nsis",
          "arch": [
            "ia32"
          ]
        }
      ]
    }
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "asar": "^0.14.6",
    "async": "^2.6.1",
    "axios": "^0.19.0",
    "body-parser": "^1.18.2",
    "electron-log": "^2.2.16",
    "electron-updater": "^4.0.6",
    "getmac": "^1.4.6",
    "highcharts": "^7.2.2",
    "jquery": "^3.5.1",
    "jquery-ui": "^1.12.1",
    "jquery-validation": "^1.17.0",
    "knex": "^0.21.1",
    "nodemailer": "^4.6.8",
    "request": "^2.87.0",
    "request-promise-native": "^1.0.5",
    "sqlite3": "^4.0.6",
    "update-electron-app": "^1.3.0",
    "uuid": "^3.3.3",
    "uuidv4": "^2.0.0",
    "xlsx": "^0.12.13",
    "electron-reload": "^1.5.0"
  },
  "devDependencies": {
    "electromon": "^1.0.10",
    "electron": "^4.0.1",
    "electron-builder": "^20.40.2",
    "electron-packager": "^15.1.0",
    "mssql": "^6.2.0",
    "mysql": "^2.18.1",
    "mysql2": "^2.1.0",
    "pg": "^8.2.1"
  }
}
